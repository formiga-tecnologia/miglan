<Actions.MigLan xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
 xsi:noNamespaceSchemaLocation="Documetation.xsd">
    <Action Name="Show" >
        <Description>Show the current state of the migration</Description>
        <Command>Show</Command>
        <Direction>1</Direction>
        <attach></attach>
        <Arguments> Show data name </Arguments>
  
    </Action>

     <Action Name="append" >
        <Description>Show the current state of the migration</Description>
        <Command>Append</Command>
        <Direction>1</Direction>
        <attach></attach>
        <Arguments> Show data name </Arguments>
     </Action>

     <Action Name="where">
        <Description>Show the current state of the migration</Description>
        <Command>where</Command>
        <Direction>-1</Direction>
        <attach></attach>
        <Arguments> Show data name </Arguments>
     </Action>

     <Action Name="More">
        <Description> Descovery More Than Values </Description>
        <Command></Command>
        <Direction>1</Direction>
        <attach>*moreThan</attach>
        <Arguments>Compare Data Set</Arguments>
     </Action>

</Actions.MigLan>